{
  "version": 2,
  "builds": [
    {
      "src": "pat-sail250.html",
      "use": "@vercel/static"
    },
    {
      "src": "images/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/",
      "dest": "/pat-sail250.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
